CREATE OR REPLACE VIEW active_students AS
SELECT 
    "Students"."id",
    "Students"."name",
    "Exams"."grade"
FROM "Students"
INNER JOIN "Exams"
    ON "Students"."id" = "Exams"."studentid"
WHERE "Exams"."grade" >= 2;

SELECT "name", "grade" FROM active_students

