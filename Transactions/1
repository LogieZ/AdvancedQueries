CREATE TABLE "Wallets" (student_id INT PRIMARY KEY, balance NUMERIC)

CREATE TABLE "Transfer_log" (
log_id SERIAL PRIMARY KEY,
from_student INT,
to_student INT,
amount NUMERIC,
status TEXT,
log_time TIMESTAMP DEFAULT CURRENT_TIMESTAMP)

